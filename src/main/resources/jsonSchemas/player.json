{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "age": {
      "type": "integer"
    },
    "gender": {
      "type": "string",
      "enum": [
        "female",
        "male"
      ]
    },
    "id": {
      "type": "integer"
    },
    "login": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "role": {
      "type": "string",
      "enum": [
        "user",
        "admin",
        "supervisor"
      ]
    },
    "screenName": {
      "type": "string"
    }
  },
  "required": [
    "age",
    "gender",
    "id",
    "login",
    "role",
    "screenName"
  ]
}