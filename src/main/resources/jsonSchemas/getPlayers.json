{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "players": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "age": {
            "type": "integer"
          },
          "gender": {
            "type": "string",
            "enum": [
              "female",
              "male"
            ]
          },
          "id": {
            "type": "integer"
          },
          "role": {
            "type": "string",
            "enum": [
              "user",
              "admin",
              "supervisor"
            ]
          },
          "screenName": {
            "type": "string"
          }
        },
        "required": [
          "age",
          "gender",
          "id",
          "role",
          "screenName"
        ]
      }
    }
  }
}